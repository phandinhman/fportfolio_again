<tr>
  <td class="text-center"></td>
  <td class="text-center">
    <%= image_tag feature.image.url, class: "img-circle", style: "width: 100px; height: 100px;" %>
  </td>
  <td><a href=""><%= feature.name %></a></td>
  <td class="text-center">
    <div class="btn-group btn-group-xs">
      <%= link_to edit_admin_feature_path(feature), title: "Edit", class: "btn btn-default", data: {toggle: "tooltip"} do %>
        <i class="fa fa-pencil"></i>
      <% end %>
      <%= link_to admin_feature_path(feature), title: "Delete",
        method: :delete, class: "btn btn-danger", data: {toggle: "tooltip", confirm: t("alert_confirm")} do %>
        <i class="fa fa-trash"></i>
      <% end %>
    </div>
  </td>
</tr>
